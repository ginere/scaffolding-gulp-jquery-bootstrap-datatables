<% // -*- javascript -*-

render={};

// Do the html clean
render.includeHtml=function(fileName){
	var html=include(fileName);

	return htmlclean(html);
};

// Include the html into js code
render.includeHtmlJs=function(fileName){
	var html=render.includeHtml(fileName);
	html=html.replace(/"/g, '\\"');
	html=html.replace(/'/g, "\\'");

	return html;
};

// Include the html into js code
render.htmlToJs=function(html){
	html=htmlclean(html);
	html=html.replace(/"/g, '\\"');
	html=html.replace(/'/g, "\\'");

	return html;
};


%>
